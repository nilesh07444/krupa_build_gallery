<!--main content start-->
<section id="main-content">
    <section class="wrapper site-min-height">
        <!-- page start-->
        <div class="row">
            <div class="col-lg-12">
                <section class="panel">
                    <header class="panel-heading">
                        Change Password
                    </header>
                    <div class="panel-body">
                        <form role="form">
                            <div class="form-group">
                                <label for="txtCurrentPassword">Current Password</label>
                                <input type="password" class="form-control" id="txtCurrentPassword" style="width:350px;" placeholder="Enter Current Password">
                            </div>
                            <div class="form-group">
                                <label for="txtNewPassword">New Password</label>
                                <input type="password" class="form-control" id="txtNewPassword" style="width:350px;" placeholder="Enter New Password">
                            </div>
                            <div class="form-group">
                                <label for="txtConfirmPassword">Confirm Password</label>
                                <input type="password" class="form-control" id="txtConfirmPassword" style="width:350px;" placeholder="Enter Confirm Password">
                            </div>
                            <button type="submit" class="btn btn-info">Submit</button>
                        </form>
                    </div>
                </section>
            </div>
        </div>
        <!-- page end-->
    </section>
</section>
<!--main content end-->
