                                    <span>Error message here</span>*@
                            </div>

                            <div class="form-group row">
                                <label for="txtLastName" class="col-xl-3 col-md-4">Last Name</label>
                                <input type="text" class="form-control col-md-8" id="txtLastName" placeholder="Enter Last Name">

                                @*<lable class="col-xl-3 col-md-4"></lable>
                                    <span>Error message here</span>*@
                            </div>

                            <div class="form-group row">
                                <label for="txtEmail" class="col-xl-3 col-md-4">Email</label>
                                <input type="text" class="form-control col-md-8" id="txtEmail" placeholder="Enter Email">

                                @*<lable class="col-xl-3 col-md-4"></lable>
                                    <span>Error message here</span>*@
                            </div>

                            <div class="form-group row">
                                <label for="txtMobileNo" class="col-xl-3 col-md-4">Mobile No</label>
                                <input type="text" class="form-control col-md-8" id="txtMobileNo" placeholder="Enter Mobile No">

                                @*<lable class="col-xl-3 col-md-4"></lable>
                                    <span>Error message here</span>*@
                            </div>

                            <button type="button" class="btn btn-primary">Submit</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Container-fluid Ends-->

</div>

